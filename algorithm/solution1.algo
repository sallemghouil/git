ALGORITHM solution1
VAR
  arr1 : ARRAY_OF INTEGER[20];
  arr2 : ARRAY_OF INTEGER[30];
 i,j,sum : INTEGER
BEGIN
read(arr1)
read(arr2)
sum :=0
FOR i FROM 0 TO arr1.length-1 STEP 1  DO
    FOR j FROM 0 TO length.arr2-1 STEP step  DO
        IF (arr1[i]=arr2[j]) THEN
            sum=sum+arr1[i]+arr2[j]
       
        END_IF
    END_FOR
END_FOR
END
